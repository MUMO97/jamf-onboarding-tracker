#!/bin/bash


DUMMY_FILE="/Users/Shared/jamfonboarding_dummy_file.txt"

echo "Dummy File for Jamf Tracking" > "$DUMMY_FILE"
echo "Created on: $(date)" >> "$DUMMY_FILE"

# Confirm the file was created
if [[ -f "$DUMMY_FILE" ]]; then
    echo "Dummy file successfully created at: $DUMMY_FILE"
else
    echo "Failed to create dummy file."
    exit 1
fi

exit 0
